{
  "Serilog": {
    "Using": ["Serilog.Sinks.DynamicSwitch"],
    "LevelSwitches": {
      "$appLogLevel": "Warning"
      //"$netLogLevel": "Information",
      //"$sysLogLevel": "Error"
    },
    //"MinimumLevel": {
    //  "ControlledBy": "$appLogLevel",
    //  "Override": {
    //    "Microsoft": "$netLogLevel",
    //    "System": "$sysLogLevel"
    //  }
    //},
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "levelSwitch": "$appLogLevel"
        }
      },
      {
        "Name": "DynamicSwitch",
        "Args": {
          "switchName": "Application",
          "levelSwitch": "$appLogLevel"
        }
      }
    ],
    "Properties": {
      "Application": "Serilog DynamicSwitch Console Sample"
    }
  }
}